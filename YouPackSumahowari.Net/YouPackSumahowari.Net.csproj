<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net8.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Authors>wh3at</Authors>
        <Copyright>© 2024 wh3at. Licensed under the MIT License.</Copyright>
        <PackageProjectUrl>https://github.com/darma-exe/YouPackSumahowari.Net</PackageProjectUrl>
        <RepositoryUrl>https://github.com/darma-exe/YouPackSumahowari.Net</RepositoryUrl>
        <Version>1.0.1</Version>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
      <DebugType>none</DebugType>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="PuppeteerSharp" Version="20.0.2" />
    </ItemGroup>

</Project>
